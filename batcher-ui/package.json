    "eslint-config-prettier": "^8.8.0",
    "gh-pages": "^3.2.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.26",
    "postcss-import": "^15.1.0",
